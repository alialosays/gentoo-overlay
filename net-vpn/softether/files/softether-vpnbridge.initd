depend() {
	need net
}

start() {
	ebegin "Starting SoftEther VPN bridge"
	/opt/softether/vpnbridge/vpnbridge start
	eend $?
}

stop() {
	ebegin "Stopping SoftEther VPN bridge"
	/opt/softether/vpnbridge/vpnbridge stop
	eend $?
}
