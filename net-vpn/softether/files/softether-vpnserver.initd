depend() {
	need net
}

start() {
	ebegin "Starting SoftEther VPN server"
	/opt/softether/vpnserver/vpnserver start
	eend $?
}

stop() {
	ebegin "Stopping SoftEther VPN server"
	/opt/softether/vpnserver/vpnserver stop
	eend $?
}
