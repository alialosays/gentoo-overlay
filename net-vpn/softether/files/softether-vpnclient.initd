depend() {
	need net
}

start() {
	ebegin "Starting SoftEther VPN client"
	/opt/softether/vpnclient/vpnclient start
	eend $?
}

stop() {
	ebegin "Stopping SoftEther VPN client"
	/opt/softether/vpnclient/vpnclient stop
	eend $?
}
